{
  "name": "guruapi",
  "version": "1.1.0",
  "description": "Backend APIs for Genshin Guru Website",
  "main": "index.js",
  "scripts": {
    "start": "node aws/layers/GuruLayer/nodejs/node_modules/main/server.js",
    "debug": "nodemon -L --watch ./aws/layers/GuruLayer/nodejs/node_modules --inspect=0.0.0.0:9222 --nolazy ./aws/layers/GuruIntDeps/node_modules/main/server.js",
    "build": "rimraf aws/layers/GuruLayer/nodejs/node_modules && tsc -p tsconfig-build.json",
    "build:watch": "rimraf aws/layers/GuruLayer/nodejs/node_modules && tsc -p tsconfig-build.json -w",
    "dev": "sucrase-node src/main/server.ts",
    "pilot": "sucrase-node index.ts",
    "gen:subvalues": "sucrase-node src/data/artifact/gen-substat-values-possibilities.ts",
    "clearjest": "jest --clearCache",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "jest:clearcache": "jest --clearCache"
  },
  "author": "Eric Oliveira",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.87",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.176",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.11.14",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.1.0",
    "faker": "^5.5.3",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "rimraf": "^3.0.2",
    "sucrase": "^3.20.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1046.0",
    "express": "^4.17.1",
    "js-combinatorics": "^1.5.4",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.4"
  }
}
